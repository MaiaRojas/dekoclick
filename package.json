{
  "name": "capitan-www",
  "version": "0.0.0",
  "main": "www/index.html",
  "scripts": {
    "build": "browserify src/index.js -o www/bundle.js -t [ babelify --presets [ es2015 react ] ] -t [ envify --NODE_ENV production ]",
    "build-css": "node-sass --include-path scss scss/main.scss www/style.css",
    "watch": "watchify src/index.js -o www/bundle.js -t [ babelify --presets [ es2015 react ] ] -t [ envify --NODE_ENV development ] -v",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Laboratoria <sumate@laboratoria.la> (http://laboratoria.la/)",
  "contributors": [
    "Lupo Montero <lupo@laboratoria.la> (https://github.com/lupomontero)",
    "Giancarlo Corzo <gian@laboratoria.la> (https://github.com/giancorzo)"
  ],
  "license": "Apache-2.0",
  "devDependencies": {
    "axios": "^0.15.3",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "babelify": "^7.3.0",
    "browserify": "^14.1.0",
    "codemirror": "^5.25.0",
    "envify": "^4.0.0",
    "node-sass": "^4.5.2",
    "react": "^15.4.2",
    "react-codemirror": "^0.3.0",
    "react-dom": "^15.4.2",
    "react-mdl": "^1.9.0",
    "react-redux": "^5.0.3",
    "react-router": "3.0.2",
    "react-router-redux": "^4.0.8",
    "redux": "^3.6.0",
    "redux-logger": "^3.0.0",
    "redux-thunk": "^2.2.0",
    "remarkable": "^1.7.1",
    "watchify": "^3.9.0"
  },
  "browserify": {
    "transform": [["babelify", { "presets": ["es2015", "react"] }]]
  }
}
